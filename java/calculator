let add = (a,b) =>
{
    console.log (add = a+b)

}



let sub = (a,b) =>
{
    console.log (sub = a-b)
}

let mul = (a,b) =>
{
    console.log (mul = a*b)
}


let div = (a,b) =>
{
    console.log (div = a/b)
}

let addition_result = add (100,200)
let subtraction_result = sub (200,100)
let multification_result = mul (92,100)
let division_result = div (200,2)





